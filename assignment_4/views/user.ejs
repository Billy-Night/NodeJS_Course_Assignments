<%- include('includes/head.ejs') %>
    <main>
        <h1>Profile</h1>
        <% if (users.length > 0) { %>
        <ul>
            <% users.forEach((user) => { %>
                <li>Name: <%= user.name %></li>
                <li>Email: <%= user.email %></li>
            <% }) %>
        </ul>
        <% } else { %>
        <h2>Sorry no user found<h2>
        <% } %>
    </main>
<%- include('includes/end.ejs')%>